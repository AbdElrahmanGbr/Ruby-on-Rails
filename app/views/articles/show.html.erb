<h1>Showing articles</h1>
<p>
  <%= link_to 'Back', articles_path %>
</p>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Created_at</th>
      <th>Updated_at</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @article.title %></td>
      <td><%= @article.body %></td>
      <td><%= @article.created_at %></td>
      <td><%= @article.updated_at %></td>
    </tr>
  </tbody>
